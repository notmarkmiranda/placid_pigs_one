Go To <a href="#<%= formatted_date_with_day(Date.today) %>">Today</a>

<ul>
<% @all_dates.each do |date| %>
  <li>
    <%= formatted_date_with_day(date) %>
    <a name="<%= formatted_date_with_day(date) %>"></a>
  </li>
  <ul>
    <% Team.all.each do |team| %>
      <li>
        <%= team.full_name %>
        <%= button_to "Winner", admin_winners_path, method: :post, params: { winner: { team_id: team.id, date: date, status: :winner } }, disabled: @winners.exists_for_date?(team, date, :winner) %>
        <%= button_to "Loser", admin_winners_path, method: :post, params: { winner: { team_id: team.id, date: date, status: :loser } }, disabled: @winners.exists_for_date?(team, date, :loser) %>
        <%= button_to "No Status", admin_winners_path, method: :post, params: { winner: { team_id: team.id, date: date, status: :no_status } }, disabled: @winners.exists_for_date?(team, date, :no_status) %>
      </li>
  <% end %>
  </ul>
<% end %>
</ul>
